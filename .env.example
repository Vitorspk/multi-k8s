# GCP Project Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=southamerica-east1
GCP_ZONE=southamerica-east1-a

# Cluster Configuration
GKE_CLUSTER_NAME=multi-k8s-cluster
GKE_ZONE=southamerica-east1-a
DEPLOYMENT_NAME=multi-k8s

# Database Configuration
POSTGRES_PASSWORD=your-secure-password
POSTGRES_USER=postgres
POSTGRES_HOST=postgres-cluster-ip-service
POSTGRES_PORT=5432
POSTGRES_DATABASE=postgres

# Redis Configuration
REDIS_HOST=redis-cluster-ip-service
REDIS_PORT=6379

# Docker Registry (will be automatically set to gcr.io/PROJECT_ID)
DOCKER_REGISTRY_PREFIX=gcr.io

# Terraform Backend
TERRAFORM_BUCKET_SUFFIX=terraform-state